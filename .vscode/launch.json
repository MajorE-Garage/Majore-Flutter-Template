    "version": "0.2.0",
    "configurations": [
        {
            "name": "Staging-Debug",
            "request": "launch",
            "type": "dart",
            "program": "lib/main/main.dart",
            "args": [
                "--flavor",
                "staging",
                "--dart-define-from-file",
                "lib/main/staging.env"
            ],
        },
        {
            "name": "Prod-Debug",
            "request": "launch",
            "type": "dart",
            "program": "lib/main/main.dart",
            "args": [
                "--flavor",
                "prod",
                "--dart-define-from-file",
                "lib/main/prod.env"
            ],
        },
        {
            "name": "Prod-Release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main/main.dart",
            "args": [
                "--flavor",
                "prod",
                "--dart-define-from-file",
                "lib/main/prod.env"
            ],
        }
    ]
}